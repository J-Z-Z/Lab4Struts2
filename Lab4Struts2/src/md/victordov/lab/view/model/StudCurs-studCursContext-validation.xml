<?xml version="1.0" encoding="GBK"?>

<!DOCTYPE validators PUBLIC 
  		"-//OpenSymphony Group//XWork Validator 1.0//EN" 
  		"http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>
	<field name="cursId">
		<field-validator type="int">
			<param name="trim">true</param>
			<message>Id Universitate este necesar</message>
		</field-validator>

		<field-validator type="regex">
			<param name="expression"><![CDATA[([\d]{1,2})]]></param>
			<param name="caseSensitive">true</param>
			<message><![CDATA[ ID-ul cursului 1 sau 2 cifre]]></message>
		</field-validator>
	</field>

	<field name="studentId">
		<field-validator type="int">
			<param name="trim">true</param>
			<message>Id Universitate este necesar</message>
		</field-validator>

		<field-validator type="regex">
			<param name="expression"><![CDATA[([\d]{1,2})]]></param>
			<param name="caseSensitive">true</param>
			<message><![CDATA[ ID-ul studentului 1 sau 2 cifre]]></message>
		</field-validator>
	</field>

</validators>  		
